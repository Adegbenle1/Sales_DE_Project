{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sales-car-adf"
		},
		"Car_Sales_Databrick_LinkedService_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Car_Sales_Databrick_LinkedService'"
		},
		"Sales_sql_Sink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sales_sql_Sink'"
		},
		"ls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake'"
		},
		"Car_Sales_Databrick_LinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0526-161607-rbkdspiw"
		},
		"Sales_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"Sales_sql_Sink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "salescarserverade.database.windows.net"
		},
		"Sales_sql_Sink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SalesCar_DB"
		},
		"Sales_sql_Sink_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminade"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://caraalesazureadelake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sales-car-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Car_Sales_Databrick_LinkedService_accessToken": {
						"value": "[parameters('Car_Sales_Databrick_LinkedService_accessToken')]"
					},
					"Sales_sql_Sink_password": {
						"value": "[parameters('Sales_sql_Sink_password')]"
					},
					"ls_datalake_accountKey": {
						"value": "[parameters('ls_datalake_accountKey')]"
					},
					"Car_Sales_Databrick_LinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Car_Sales_Databrick_LinkedService_properties_typeProperties_existingClusterId')]"
					},
					"Sales_github_properties_typeProperties_url": {
						"value": "[parameters('Sales_github_properties_typeProperties_url')]"
					},
					"Sales_sql_Sink_properties_typeProperties_server": {
						"value": "[parameters('Sales_sql_Sink_properties_typeProperties_server')]"
					},
					"Sales_sql_Sink_properties_typeProperties_database": {
						"value": "[parameters('Sales_sql_Sink_properties_typeProperties_database')]"
					},
					"Sales_sql_Sink_properties_typeProperties_userName": {
						"value": "[parameters('Sales_sql_Sink_properties_typeProperties_userName')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}